{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "users/discord/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputUsers",
      "databaseName": "cyberskies",
      "collectionName": "users",
      "connectionStringSetting": "AzureWebJobsStorage",
      "sqlQuery": "SELECT * FROM c WHERE c.discord.id={id}"
    }
  ]
}