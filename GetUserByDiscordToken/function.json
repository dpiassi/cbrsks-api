{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "user/discord/{token}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputUsers",
      "databaseName": "cyberskies",
      "collectionName": "users",
      "connectionStringSetting": "CosmoDB",
      "sqlQuery": "SELECT c.id, c.discord FROM c WHERE c.discord.token={token}"
    }
  ]
}