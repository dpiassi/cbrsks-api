{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "auth/discord/refresh/{token}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputUsers",
      "databaseName": "cyberskies",
      "collectionName": "users",
      "connectionStringSetting": "AzureWebJobsStorage",
      "sqlQuery": "SELECT c.id, c.discord FROM c WHERE c.discord.token={token}"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputUsers",
      "databaseName": "cyberskies",
      "collectionName": "users",
      "createIfNotExists": true,
      "connectionStringSetting": "AzureWebJobsStorage"
    }
  ]
}